<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<!-- listener added for adding TimerMBeanScheduler as a startup class-->
<web-app>
  <context-param>
   <param-name>filename</param-name>
  <param-value>cis.properties</param-value>
  </context-param>
 <listener>
 <listener-class>com.equant.csi.weblogic.MBeanRegistrationListener</listener-class>
</listener>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>